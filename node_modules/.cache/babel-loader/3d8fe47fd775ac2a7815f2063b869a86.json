nd-size: cover;\r\n        background-position: center;\r\n      }\r\n      div.des{\r\n        box-sizing: border-box;\r\n        width: 50%;\r\n        padding-left: 1%;\r\n      }\r\n    }\r\n\r\n    .r-b{\r\n      position: absolute;\r\n      right: 20px;\r\n      bottom: 20px;\r\n      p{\r\n        position: relative;\r\n        top: -10px;\r\n        color: #aaa;\r\n        font-size: 12px;\r\n      }\r\n      a{\r\n        padding: 10px 20px;\r\n        background-color: #409eff;\r\n        color: #fff;\r\n        transition: opacity .3s;\r\n        &:hover{\r\n          opacity: .8;\r\n        }\r\n      }\r\n    }\r\n\r\n  }\r\n\r\n\r\n}\r\n</style>\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n"],"sourceRoot":"src/views"}]}