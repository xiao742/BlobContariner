    <div\r\n        v-else\r\n        class=\"user\"\r\n      >\r\n        <el-button\r\n          class=\"login\"\r\n          type=\"success\"\r\n          size=\"mini\"\r\n          @click=\"handleLogin\"\r\n        >登陆 / 注册\r\n        </el-button>\r\n      </div>\r\n    </div>\r\n\r\n    <!--登陆与注册的弹窗-->\r\n    <Login\r\n      v-show=\"ifShowLogin\"\r\n      @close=\"handleClose\"\r\n    ></Login>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport * as THREE from \"three\";\r\nimport WAVES from \"vanta/src/vanta.waves\";\r\nimport Login from \"@/components/Login\";\r\nimport Face from \"@/components/Face\";\r\nimport { mapState } from \"vuex\";\r\n\r\nexport default {\r\n  name: \"AppNav\",\r\n  components: {\r\n    Login,\r\n    Face,\r\n  },\r\n  data() {\r\n    return {\r\n      ifShowLogin: false,\r\n    };\r\n  },\r\n  mounted() {\r\n    this.vantaEffect = WAVES({\r\n      el: this.$refs.vantaRef,\r\n      THREE: THREE,\r\n    });\r\n    VANTA.WAVES({\r\n      el: this.$refs.vantaRef,\r\n      /*以下为样式配置*/\r\n      mouseControls: true,\r\n      touchControls: true,\r\n      gyroControls: false,\r\n      minHeight: 200.0,\r\n      minWidth: 200.0,\r\n      scale: 1.0,\r\n      scaleMobile: 1.0,\r\n      color: 0x5f88,\r\n      shininess: 76.0,\r\n      waveHeight: 12.0,\r\n      waveSpeed: 0.95,\r\n      zoom: 0.89,\r\n    });\r\n  },\r\n  beforeDestroy() {\r\n    if (this.vantaEffect) {\r\n      this.vantaEffect.destroy();\r\n    }\r\n  },\r\n  computed: {\r\n    ...mapState([\"userInfo\"]),\r\n  },\r\n  methods: {\r\n    //登陆按钮点击\r\n    handleLogin() {\r\n      this.ifShowLogin = true;\r\n    },\r\n\r\n    //登陆弹窗关闭\r\n    handleClose() {\r\n      this.ifShowLogin = false;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n#AppNav {\r\n  width: 100%;\r\n  height: 86px;\r\n  background-color: #fff;\r\n  box-shadow: 0 0 2px #999;\r\n\r\n  .content {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    box-sizing: border-box;\r\n    width: 100%;\r\n    height: 100%;\r\n    max-width: 1300px;\r\n    align-items: center;\r\n    padding: 0 15px;\r\n    //background-color: pink;\r\n    margin: 0 auto;\r\n\r\n    /*logo*/\r\n\r\n    .logo {\r\n      width: 130px;\r\n      height: 50px;\r\n\r\n      a {\r\n        display: block;\r\n        width: 100%;\r\n        height: 100%;\r\n        line-height: 42px;\r\n        font-size: 26px;\r\n        letter-spacing: 5px;\r\n        font-family: \"Pacifico\", \"serif\";\r\n        color: rgba(0, 0, 0, 0.7);\r\n      }\r\n    }\r\n\r\n    /*导航*/\r\n\r\n    .nav {\r\n      width: 470px;\r\n      height: 50px;\r\n      //background-color: aquamarine;\r\n      a {\r\n        float: left;\r\n        display: block;\r\n        height: 98%;\r\n        border-bottom: 2px solid transparent;\r\n        padding: 0 25px;\r\n        color: #777;\r\n        line-height: 48px;\r\n        font-weight: bolder;\r\n        font-size: 20px;\r\n        transition: all 0.3s;\r\n\r\n        &.router-link-exact-active {\r\n          color: #00b7ff;\r\n          border-color: #00b7ff;\r\n        }\r\n\r\n        &:hover {\r\n          color: #00b7ff;\r\n        }\r\n      }\r\n    }\r\n\r\n    /*登陆与注册*/\r\n\r\n    .user {\r\n      height: 50px;\r\n      //background-color: antiquewhite;\r\n      .el-button {\r\n        display: block;\r\n        margin-top: 11px;\r\n      }\r\n\r\n      .login {\r\n        float: left;\r\n      }\r\n\r\n      .register {\r\n        float: right;\r\n      }\r\n    }\r\n  }\r\n}\r\n</style>\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n"],"mappings":"AA2CA,YAAAA,KAAA;AACA,OAAAC,KAAA;AACA,OAAAC,KAAA;AACA,OAAAC,IAAA;AACA,SAAAC,QAAA;AAEA;EACAC,IAAA;EACAC,UAAA;IACAJ,KAAA;IACAC;EACA;EACAI,KAAA;IACA;MACAC,WAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,WAAA,GAAAT,KAAA;MACAU,EAAA,OAAAC,KAAA,CAAAC,QAAA;MACAb,KAAA,EAAAA;IACA;IACAc,KAAA,CAAAb,KAAA;MACAU,EAAA,OAAAC,KAAA,CAAAC,QAAA;MACA;MACAE,aAAA;MACAC,aAAA;MACAC,YAAA;MACAC,SAAA;MACAC,QAAA;MACAC