CA;AACA,SAAA,OAAA;AACA;AA3CA,CAAA","sourcesContent":["<template>\r\n  <div id=\"ArticleID\" v-if=\"d.title\">\r\n    <h1>{{d.title}}</h1>\r\n    <p class=\"author-time\">\r\n      作者：{{d.author.user}} 时间：{{d.time | fmtTime}}\r\n    </p>\r\n    <div id=\"article-content\" ref=\"mdToHTML\"></div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"ArticleID\",\r\n  data(){\r\n    return {\r\n      d: {}\r\n    }\r\n  },\r\n  watch:{\r\n    \"$route\": function(){\r\n      this.refresh()\r\n    }\r\n  },\r\n  methods:{\r\n    async refresh(){\r\n      // 知道文章的id，以便于向后端查询数据\r\n      let id = this.$route.params.id\r\n      //向后端查询文章id对应的信息\r\n      let res = await this.$axios({\r\n        method: 'get',\r\n        url: \"/art/id\",\r\n        params: {id}\r\n      })\r\n\r\n      this.d = res.data.data\r\n\r\n      let {data} = await this.$axios({\r\n        method: 'get',\r\n        url: this.d.src\r\n      })\r\n      this.$refs.mdToHTML.innerHTML = \"\"\r\n      editormd.markdownToHTML(\"article-content\",{\r\n        markdown: data,\r\n        toc: false,\r\n        emoji: true,\r\n        taskList        : true,\r\n        tex             : true,\r\n        flowChart       : true,\r\n        sequenceDiagram : true\r\n      })\r\n    }\r\n  },\r\n  mounted() {\r\n    this.refresh()\r\n  },\r\n\r\n}\r\n</script>\r\n\r\n<style scoped lang=\"less\">\r\n#ArticleID {\r\n  position: relative;\r\n  box-sizing: border-box;\r\n  overflow: hidden;\r\n  width: 100%;\r\n  margin-bottom: 15px;\r\n  box-shadow: 0 0 4px #ddd;\r\n  padding: 15px;\r\n  background-color: #fff;\r\n  font-family: \"Quicksand\",\"Microsoft YaHei\",sans-serif;\r\n  h1{\r\n    text-align: center;\r\n  }\r\n  .author-time{\r\n    text-align: center;\r\n    font-size: 12px;\r\n    font-family: \"Microsoft YaHei\",\"sans-serif\";\r\n    color: #aaa;\r\n  }\r\n  #article-content{\r\n    max-width: 100% !important;\r\n    padding: 0 !important;\r\n  }\r\n}\r\n</style>\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n"],"sourceRoot":"src/views"}]}