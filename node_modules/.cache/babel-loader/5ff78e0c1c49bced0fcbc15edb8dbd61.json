 {\r\n            required: true,\r\n            trigger: \"blur\",\r\n            message: \"请输入图标链接\",\r\n          },\r\n        ],\r\n        des: [\r\n          {\r\n            required: true,\r\n            trigger: \"blur\",\r\n            message: \"请输入描述\",\r\n          },\r\n        ],\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    async handleSubmit(formName) {\r\n      this.$refs[formName].validate(async (valid) => {\r\n        if (!valid) {\r\n          return;\r\n        }\r\n        let { data } = await this.$axios({\r\n          method: \"post\",\r\n          url: \"/admin/addLink\",\r\n          data: {\r\n            src: this.form.src,\r\n            icon: this.form.icon,\r\n            name: this.form.name,\r\n            des: this.form.des,\r\n          },\r\n        });\r\n\r\n        if (data.code !== 0) return;\r\n\r\n        this.$message.success(\"添加成功\");\r\n        this.$router.push(\"/link\");\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.form {\r\n  .el-button {\r\n    width: 100%;\r\n  }\r\n}\r\n</style>\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n"],"sourceRoot":"src/components/admin"}]}